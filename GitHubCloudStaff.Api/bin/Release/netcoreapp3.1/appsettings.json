{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ApplicationSettings": {
    "AppSettings": {
      "ByUser": "https://api.github.com/users",
      "ByUserDetail": "https://api.github.com/users/",
      "ListCount": "5"
    }
  },

  "AllowedHosts": "*",

  "SwaggerOptions": {
    "JsonRoute": "swagger/{documentname}/swagger.json",
    "Desription": "Git Hub User Account",
    "UIEndpoint": "v1.0/swagger.json"
  }

}
